{
  "include": ["src"],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "tests_quarantine",
    ".venv",
    "venv",
    "src/tensorguard/platform/models",
    "src/tensorguard/platform/auth.py",
    "src/tensorguard/platform/database.py",
    "src/tensorguard/platform/tg_tinker_api/models.py",
    "src/tensorguard/tgsp/container.py"
  ],
  "pythonVersion": "3.9",
  "pythonPlatform": "Linux",
  "typeCheckingMode": "basic",
  "reportMissingImports": false,
  "reportMissingTypeStubs": false,
  "reportOptionalMemberAccess": false,
  "reportOptionalSubscript": false,
  "reportGeneralTypeIssues": "warning",
  "reportUnusedImport": false,
  "reportUnusedVariable": false,
  "reportPrivateUsage": false,
  "reportConstantRedefinition": false,
  "reportIncompatibleMethodOverride": false,
  "reportIncompatibleVariableOverride": false,
  "reportUntypedFunctionDecorator": false,
  "reportUntypedClassDecorator": false,
  "reportUntypedBaseClass": false,
  "reportUntypedNamedTuple": false,
  "reportPropertyTypeMismatch": false,
  "reportFunctionMemberAccess": false,
  "reportInvalidStringEscapeSequence": "error",
  "reportUnknownParameterType": false,
  "reportUnknownArgumentType": false,
  "reportUnknownLambdaType": false,
  "reportUnknownVariableType": false,
  "reportUnknownMemberType": false,
  "reportMissingParameterType": false,
  "reportMissingTypeArgument": false,
  "reportCallInDefaultInitializer": false,
  "reportUnnecessaryIsInstance": false,
  "reportUnnecessaryCast": false,
  "reportAssertAlwaysTrue": "warning",
  "reportSelfClsParameterName": "warning",
  "reportImplicitStringConcatenation": false,
  "reportReturnType": false,
  "reportArgumentType": false,
  "strictListInference": false,
  "strictDictionaryInference": false,
  "strictSetInference": false,
  "strictParameterNoneValue": false
}
